{
  "name": "ska-webapps-react-skeleton",
  "version": "0.0.0",
  "private": true,
  "description": "SKA React Web App Skeleton",
  "repository": "git@gitlab.com:ska-telescope/templates/ska-react-webapp-skeleton.git",
  "license": "BSD 3-Clause",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "checker": "yarn prettier:fix && yarn lint:fix",
    "code-analysis": "eslint src/**/*.{js,jsx,ts,tsx} --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:component": "cypress run --component --config video=false",
    "dev": "ENV_TYPE_FILE=env_scripts/env_config ENV_JS_OUTPUT_LOCATION=public/env.js bash env_scripts/env_config.sh js && webpack serve --mode development --open",
    "instrument": "webpack serve --mode development @cypress/instrument-cra",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "skao:update": "yarn add @ska-telescope/ska-gui-components@latest @ska-telescope/ska-gui-local-storage@latest @ska-telescope/ska-login-page@latest",
    "start": "ENV_TYPE_FILE=env_scripts/env_config ENV_JS_OUTPUT_LOCATION=public/env.js bash env_scripts/env_config.sh js && NODE_ENV=test webpack serve --mode development --open",
    "test:component:headless": "cypress run --component --headless --config video=false",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:cypress:component:ci": "cypress run --component --headless --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml --config video=false",
    "test:cypress:e2e:ci": "cypress run --e2e --headless --reporter junit --config video=false --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:e2e:headless": "cypress run --e2e --headless --config video=false",
    "test:headless": "yarn cypress:run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "**/**/json5": "^1.0.2",
    "@babel/traverse": "^7.25.0",
    "minimatch": "^3.0.5",
    "debug": "^4.3.1",
    "babel-traverse": "^6.26.0"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.0.2",
    "@azure/msal-react": "^3.0.2",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.1.10",
    "@ska-telescope/ska-gui-components": "^2.2.2",
    "@ska-telescope/ska-gui-local-storage": "^2.0.4",
    "@ska-telescope/ska-login-page": "^0.9.20",
    "babel-core": "6",
    "bundle-loader": "^0.5.6",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv-webpack": "^8.1.0",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "json-loader": "^0.5.7",
    "moment": "^2.29.1",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-awesome-button": "^7.0.5",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-select": "^5.10.1",
    "react-tabs": "^6.1.0",
    "react-test-renderer": "^18.2.0",
    "react-toggle": "^4.1.3",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/runtime": "^7.17.9",
    "@cypress/code-coverage": "^3.11.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^7.1.2",
    "cypress": "^13.6.6",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "istanbul-lib-coverage": "^3.2.0",
    "junit": "^1.4.9",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.49.4",
    "sass-loader": "^14.1.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
